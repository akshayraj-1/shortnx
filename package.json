{
  "name": "shortnx",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently \"set NODE_ENV=development && node src/index.js\" \"npm run watch\"",
    "start": "set NODE_ENV=production && node src/index.js",
    "start2": "concurrently \"set NODE_ENV=production && node src/index.js\" \"npm run tailwind\"",
    "build": "postcss src/views/input.css -o public/css/output.css",
    "watch": "postcss src/views/input.css -o public/css/output.css --watch",
    "tailwind": "npx tailwindcss -i src/views/input.css -o public/css/output.css --watch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "ejs-mate": "^4.0.0",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.0",
    "jsonwebtoken": "^9.0.2",
    "metadata-scraper": "^0.2.61",
    "mongoose": "^8.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cssnano": "^7.0.6",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-nested": "^7.0.2",
    "tailwindcss": "^3.4.16"
  }
}
